generator client {
  provider = "prisma-client"
  output   = "./generated"
}

datasource db {
  provider = "postgresql"
}


model Animal {
    id String @id @default(uuid())
    name String
    birthDate String
    identification Int?

    // Audite

    createdAt DateTime @default(now())
    updatedAt DateTime?
    isDeleted Boolean @default(false)

    // Relation

}

model Veterinarian {
    id String @id @default(uuid())
    first_name String
    last_name String
    email String @unique
    phone String @unique
    licenseNumber String @unique

    // Audite

    createdAt DateTime @default(now())
    updatedAt DateTime?
    isDeleted Boolean @default(false)

    // Relation

}